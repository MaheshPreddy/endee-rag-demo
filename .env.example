# Environment variables template
# Copy this file to .env and fill in your values

# Required: OpenAI API Key (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-your-key-here

# Optional: Endee Vector Database Configuration
# Use this if you have a running Endee server (docker-compose or remote)
ENDEE_BASE_URL=http://localhost:8080
ENDEE_API_KEY=
ENDEE_COLLECTION=docs

# Optional: API Configuration
API_BASE_URL=http://localhost:8000
